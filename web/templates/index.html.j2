<!doctype html>
<html lang="en">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <link rel="stylesheet" href="../templates/src/bootstrap.min.css">

</head>

<body>
  <div class="container px-2 mt-5">
    <h1 class="display-3 fw-bold text-center">Mantid Unit Test Dashboard</h1>
    <div class="col-lg-12 mx-auto">
      <h2 class="display-5 text-center">Combined Results</h2>
      <div class="col-lg-9 mx-auto text-center">
        <ul class="list-group">
            {% for os in os_table_html_locations.keys() %}
              <a href="{{ os_table_html_locations[os] }}" class="list-group-item list-group-item-action">{{ os }}</a>
            {% endfor %}
        </ul>
      </div>
    </div>
  </div>
</body>